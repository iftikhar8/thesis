pages = {163--173},
annote = {ISI Document Delivery No.: 354VFTimes Cited: 110Cited Reference Count: 39Fisher, R Bellwood, DR Job, SDInter-researchOldendorf luhe},
affiliation = {James Cook Univ N Queensland, Dept Marine Biol, Townsville, Qld 4811, Australia.Fisher, R (reprint author), James Cook Univ N Queensland, Dept Marine Biol, Townsville, Qld 4811, Australia.rebecca.fisher@jcu.edu.au},
keywords = {connectivity, coral reef fish, coral-reefs, Development, Dispersal, GREAT-BARRIER-REEF, horizontal distribution, LARVAE, lizard-island, management, morphology, pelagic, POPULATIONS, presettlement, retention, stages, swimming},
