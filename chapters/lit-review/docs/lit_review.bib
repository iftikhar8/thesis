@article{Siegel:2008km,
author = {Siegel, David A and Mitarai, Satoshi and Costello, Christopher and Gaines, Steven D and Kendall, Bruce E and Warner, Robert R and Winters, K B},
title = {{The stochastic nature of larval connectivity among nearshore marine populations}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2008},
volume = {105},
number = {26},
pages = {8974--8979},
month = jul,
annote = {Siegel, D. A. Mitarai, S. Costello, C. J. Gaines, S. D. Kendall, B. E. Warner, R. R. Winters, K. B.},
doi = {10.1073/pnas.0802544105},
language = {English},
rating = {0},
date-added = {2013-08-21T06:36:58GMT},
date-modified = {2016-06-07T03:57:44GMT},
abstract = {Many nearshore fish and invertebrate populations are overexploited even when apparently coherent management structures are in place. One potential cause of mismanagement may be a poor understanding and accounting of stochasticity, particularly for stock recruitment. Many of the fishes and invertebrates that comprise nearshore fisheries are relatively sedentary as adults but have an obligate larval pelagic stage that is dispersed by ocean currents. Here, we demonstrate that larval connectivity is inherently an intermittent and heterogeneous process on annual time scales. This stochasticity arises from the advection of pelagic larvae by chaotic coastal circulations. This result departs from typical assumptions where larvae simply diffuse from one site to another or where complex connectivity patterns are created by transport within spatially complicated environments. We derive a statistical model for the expected variability in larval settlement patterns and demonstrate how larval connectivity varies as a function of different biological and physical processes. The stochastic nature of larval connectivity creates an unavoidable uncertainty in the assessment of fish recruitment and the resulting forecasts of sustainable yields.},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.0802544105},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/03/03BFCF3F-BA85-4633-93D3-CAD3F8F0866C.pdf},
file = {{03BFCF3F-BA85-4633-93D3-CAD3F8F0866C.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/03/03BFCF3F-BA85-4633-93D3-CAD3F8F0866C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.0802544105}}
}

