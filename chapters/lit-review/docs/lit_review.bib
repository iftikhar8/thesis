@article{Bode:2011hj,
author = {Bode, Michael and Bode, L and Armsworth, Paul R},
title = {{Different dispersal abilities allow reef fish to coexist}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2011},
volume = {108},
number = {39},
pages = {16317--16321},
month = sep,
publisher = {National Acad Sciences},
affiliation = {Australian Research Council Centre of Excellence for Environmental Decisions, School of Botany, University of Melbourne, Parkville, Victoria 3010, Australia. mbode@unimelb.edu.au},
doi = {10.1073/pnas.1101019108},
pmid = {21930902},
pmcid = {PMC3182745},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-02-04T07:42:51GMT},
date-modified = {2016-06-07T03:57:14GMT},
abstract = {The coexistence of multiple species on a smaller number of limiting resources is an enduring ecological paradox. The mechanisms that maintain such biodiversity are of great interest to ecology and of central importance to conservation. We describe and prove a unique and robust mechanism for coexistence: Species that differ only in their dispersal abilities can coexist, if habitat patches are distributed at irregular distances. This mechanism is straightforward and ecologically intuitive, but can nevertheless create complex coexistence patterns that are robust to substantial environmental stochasticity. The Great Barrier Reef (GBR) is noted for its diversity of reef fish species and its complex arrangement of reef habitat. We demonstrate that this mechanism can allow fish species with different pelagic larval durations to stably coexist in the GBR. Further, coexisting species on the GBR often dominate different subregions, defined primarily by cross-shelf position. Interspecific differences in dispersal ability generate similar coexistence patterns when dispersal is influenced by larval behavior and variable oceanographic conditions. Many marine and terrestrial ecosystems are characterized by patchy habitat distributions and contain coexisting species that have different dispersal abilities. This coexistence mechanism is therefore likely to have ecological relevance beyond reef fish.},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1101019108},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/5E/5E73337E-C77D-4700-9E70-43433F070CAC.pdf},
file = {{5E73337E-C77D-4700-9E70-43433F070CAC.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/5E/5E73337E-C77D-4700-9E70-43433F070CAC.pdf:application/pdf;5E73337E-C77D-4700-9E70-43433F070CAC.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/5E/5E73337E-C77D-4700-9E70-43433F070CAC.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1101019108}}
}

@article{Garavelli:2016cp,
author = {Garavelli, Lysel and Colas, Fran{\c c}ois and Verley, Philippe and Kaplan, David Michael and Yannicelli, Beatriz and Lett, Christophe},
title = {{Influence of Biological Factors on Connectivity Patterns for Concholepas concholepas (loco) in Chile.}},
journal = {PLoS ONE},
year = {2016},
volume = {11},
number = {1},
pages = {e0146418},
affiliation = {Institut de Recherche pour le D{\'e}veloppement (IRD), UMI 209 UPMC UMMISCO, Centre de Recherche Halieutique M{\'e}diterran{\'e}enne et Tropicale, S{\`e}te, France.},
doi = {10.1371/journal.pone.0146418},
pmid = {26751574},
pmcid = {PMC4713471},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-01-19T04:27:01GMT},
date-modified = {2016-01-25T02:05:44GMT},
abstract = {In marine benthic ecosystems, larval connectivity is a major process influencing the maintenance and distribution of invertebrate populations. Larval connectivity is a complex process to study as it is determined by several interacting factors. Here we use an individual-based, biophysical model, to disentangle the effects of such factors, namely larval vertical migration, larval growth, larval mortality, adults fecundity, and habitat availability, for the marine gastropod Concholepas concholepas (loco) in Chile. Lower transport success and higher dispersal distances are observed including larval vertical migration in the model. We find an overall decrease in larval transport success to settlement areas from northern to southern Chile. This spatial gradient results from the combination of current direction and intensity, seawater temperature, and available habitat. From our simulated connectivity patterns we then identify subpopulations of loco along the Chilean coast, which could serve as a basis for spatial management of this resource in the future.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=26751574&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/8B/8BAD4D5B-B1D2-40F2-82C2-331F77CFE803.pdf},
file = {{8BAD4D5B-B1D2-40F2-82C2-331F77CFE803.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/8B/8BAD4D5B-B1D2-40F2-82C2-331F77CFE803.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0146418}}
}

