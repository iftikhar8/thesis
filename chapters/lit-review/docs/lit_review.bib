affiliation = {[Almany, G. R.; Connolly, S. R.; Jones, G. P.; Mills, M.; Pressey, R. L.; Williamson, D. H.] James Cook Univ, Sch Marine {\&} Trop Biol, Townsville, Qld 4811, Australia. [Almany, G. R.; Connolly, S. R.; Jones, G. P.; Mills, M.; Pressey, R. L.; Williamson, D. H.] James Cook Univ, ARC Ctr Excellence Coral Reef Studies, Townsville, Qld 4811, Australia. [Heath, D. D.; Hogan, J. D.] Univ Windsor, Dept Biol Sci, Great Lakes Inst Environm Res, Windsor, ON N9B 3P4, Canada. [McCook, L. J.] Great Barrier Reef Marine Pk Author, Townsville, Qld 4810, Australia.Almany, GR (reprint author), James Cook Univ, Sch Marine {\&} Trop Biol, Townsville, Qld 4811, Australia.glenn.almany@jcu.edu.au},
keywords = {adult census size, Cod, Conservation planning, Dispersal, effective population-size, fish populations, gadus-morhua, genetic-structure, GREAT-BARRIER-REEF, larval dispersal, management, Marine, propagule, protected area, Protected areas, Resilience, Risk-spreading, spawning aggregations},
