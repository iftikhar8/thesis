@article{Game:2009jz,
author = {Game, E T and Bode, Michael and McDonald-Madden, E and Grantham, H S and Possingham, Hugh P},
title = {{Dynamic marine protected areas can improve the resilience of coral reef systems}},
journal = {Ecology letters},
year = {2009},
volume = {12},
number = {12},
pages = {1336--1346},
month = dec,
annote = {ISI Document Delivery No.: 517QJTimes Cited: 10Cited Reference Count: 50Game, Edward T. Bode, Michael McDonald-Madden, Eve Grantham, Hedley S. Possingham, Hugh P.Australian Government Department of Environment, Water, Heritage and the Arts; CSIRO Wealth from Oceans AwardThis work was supported by the Centre for Applied Environmental Decision Analysis, a Commonwealth Environmental Research Facility Hub funded by the Australian Government Department of Environment, Water, Heritage and the Arts (HPP), and by a CSIRO Wealth from Oceans Award (ETG). We thank Alison Green, Fiorenza Micheli, Enric Sala, and four anonymous referees for helpful comments on this manuscript.Wiley-blackwell publishing, incMalden},
affiliation = {[Game, Edward T.; McDonald-Madden, Eve; Grantham, Hedley S.; Possingham, Hugh P.] Univ Queensland, Ctr Appl Environm Decis Anal, St Lucia, Qld 4072, Australia. [Game, Edward T.] Nature Conservancy, W End, Qld 4101, Australia. [Bode, Michael] Univ Melbourne, Ctr Appl Environm Decis Anal, Sch Bot, Parkville, Vic 3010, Australia.Game, ET (reprint author), Univ Queensland, Ctr Appl Environm Decis Anal, St Lucia, Qld 4072, Australia.egame@tnc.org},
keywords = {adaptive management, CLIMATE-CHANGE, communities, CONSERVATION, coral-reefs, dynamic management, Fish, GREAT-BARRIER-REEF, MARINE RESERVES, papua-new-guinea, periodic closures, phase-shifts, recovery, reserves, Resilience},
doi = {10.1111/j.1461-0248.2009.01384.x},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-08-21T06:36:48GMT},
date-modified = {2016-06-07T03:57:14GMT},
abstract = {Marine Protected Areas are usually static, permanently closed areas. There are, however, both social and ecological reasons to adopt dynamic closures, where reserves move through time. Using a general theoretical framework, we investigate whether dynamic closures can improve the mean biomass of herbivorous fishes on reef systems, thereby enhancing resilience to undesirable phase-shifts. At current levels of reservation (10-30%), moving protection between all reefs in a system is unlikely to improve herbivore biomass, but can lead to a more even distribution of biomass. However, if protected areas are rotated among an appropriate subset of the entire reef system (e.g. rotating 10 protected areas between only 20 reefs in a 100 reef system), dynamic closures always lead to increased mean herbivore biomass. The management strategy that will achieve the highest mean herbivore biomass depends on both the trajectories and rates of population recovery and decline. Given the current large-scale threats to coral reefs, the ability of dynamic marine protected areas to achieve conservation goals deserves more attention.},
url = {http://doi.wiley.com/10.1111/j.1461-0248.2009.01384.x},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/C6/C6AA7A70-F415-4362-A4A5-B335ECB6813C.pdf},
file = {{C6AA7A70-F415-4362-A4A5-B335ECB6813C.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/C6/C6AA7A70-F415-4362-A4A5-B335ECB6813C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1111/j.1461-0248.2009.01384.x}}
}

@article{Putman:2016cq,
author = {Putman, Nathan F and Lumpkin, Rick and Sacco, Alexander E and Mansfield, Katherine L},
