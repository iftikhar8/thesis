annote = {ISI Document Delivery No.: YQ193Times Cited: 135Cited Reference Count: 22Leis, JM Carson-Ewart, BMInter-researchOldendorf luhe},
affiliation = {Australian Museum, Div Vertebrate Zool, Fish Sect, Sydney, NSW 2000, Australia. Australian Museum, Ctr Biodivers {\&} Conservat Res, Sydney, NSW 2000, Australia.Leis, JM (reprint author), Australian Museum, Div Vertebrate Zool, Fish Sect, 6 Coll St, Sydney, NSW 2000, Australia.},
