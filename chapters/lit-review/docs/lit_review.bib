file = {{4F075DD7-E7F1-4AFE-B5DE-7D0224FF61E1.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/4F/4F075DD7-E7F1-4AFE-B5DE-7D0224FF61E1.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.3354/meps252239}}
}

