annote = {ISI Document Delivery No.: 231VYTimes Cited: 36Cited Reference Count: 103Miller, Thomas J.Workshop on Advancements in Modelling Physical-Biological Interactions in Fish Early-Life HistoryApr, 2006Nantes, FRANCEWGPBI, ICES Working Grp Recruitment ProcInter-researchOldendorf luhe},
affiliation = {Univ Maryland, Ctr Environm Sci, Chesapeake Biol Lab, Solomons, MD 20688 USA.Miller, TJ (reprint author), Univ Maryland, Ctr Environm Sci, Chesapeake Biol Lab, PO Box 38, Solomons, MD 20688 USA.miller@cbl.umces.edu},
keywords = {BEHAVIOR, cod gadus-morhua, EARLY-LIFE-HISTORY, eastern north-atlantic, feeding, Fish, georges bank, growth, haddock melanogrammus-aeglefinus, individual-based models, larval fish, mackerel scomber-scombrus, mortality, physical-biological models, POPULATIONS, RECRUITMENT, sardinops-melanostictus, southern benguela, spatially-explicit},
doi = {10.3354/meps06973},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-08-21T06:36:47GMT},
date-modified = {2016-12-15T04:20:09GMT},
abstract = {Annual publications involving the application of coupled physical-biological models for understanding fish recruitment processes have increased over the last decade. Sixty-nine papers were reviewed to assess the contribution these models have made to recruitment prediction and understanding. The majority of models reviewed were 2- and 3-dimensional numerical simulation models, although a limited number of I-dimensional analytical models were included. Most models used a Lagrangian tracking algorithm to advect and diffuse particles within the model domain. The vertical and horizontal resolutions and temporal durations of the models varied widely. This review identified 3 categories of papers: explanatory, inferential and hypothesis generating. Reviewed papers were dominated by explanatory approaches. Assessment of the sensitivity of model predictions to the model parameters were rare, but not entirely absent in this group of papers. Inferential approaches were the next most common, and sought to infer the presence or role of a particular mechanism. Hypothesis-generating publications were the rarest, but perhaps have the most to contribute to a greater understanding of recruitment processes. An increase in the frequency of hypothesis-generating applications of coupled physical-biological models may be expected over time as the field matures and refinements to both the physical and biological processes included in the models are made.},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/F7/F73510CD-5B0E-48B4-931D-203AD5312B97.pdf},
file = {{F73510CD-5B0E-48B4-931D-203AD5312B97.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/F7/F73510CD-5B0E-48B4-931D-203AD5312B97.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.3354/meps06973}}
}

