@article{Foster:2012ib,
author = {Foster, Nicola L and Paris, Claire B and Kool, Johnathan T and Baums, Iliana B and Stevens, Jamie R and Sanchez, Juan A and Bastidas, Carolina and Agudelo, Claudia and Bush, Phillippe and Day, Owen and Ferrari, Renata and Gonzalez, Patricia and Gore, Shannon and Guppy, Reia and McCartney, Michael A and McCoy, Croy and Mendes, Judith and Srinivasan, Ashwanth and Steiner, Sascha and Vermeij, Mark J A and Weil, Ernesto and Mumby, Peter J},
title = {{Connectivity of Caribbean coral populations: complementary insights from empirical and modelled gene flow.}},
journal = {Molecular Ecology},
year = {2012},
volume = {21},
number = {5},
pages = {1143--1157},
month = mar,
annote = {ISI Document Delivery No.: 897YPTimes Cited: 0Cited Reference Count: 72Foster, Nicola L. Paris, Claire B. Kool, Johnathan T. Baums, Iliana B. Stevens, Jamie R. Sanchez, Juan A. Bastidas, Carolina Agudelo, Claudia Bush, Phillippe Day, Owen Ferrari, Renata Gonzalez, Patricia Gore, Shannon Guppy, Reia McCartney, Michael A. McCoy, Croy Mendes, Judith Srinivasan, Ashwanth Steiner, Sascha Vermeij, Mark J. A. Weil, Ernesto Mumby, Peter J.NERC[NE/E010393/1]; GEF Coral Reef Targeted Research; EPA Star; NSF[OCE0550732, OCE0928423, OCE-0550294, OCE-0825979]We thank NERC for funding this project (NE/E010393/1) through a grant to PJM, JRS and CBP, and the GEF Coral Reef Targeted Research and an EPA Star grant for helping establish the collaborations needed to undertake the study. The Colombian government granted access to genetic resources to J.A.S. (Contract 007, resolution 634, 14 March 2007). C.B.P is funded by NSF OCE0550732, OCE0928423. IBB received support from NSF OCE-0550294 and OCE-0825979.Wiley-blackwellMalden},
affiliation = {Marine Spatial Ecology Lab, School of Biosciences, University of Exeter, Prince of Wales Road, Exeter, UK.},
keywords = {Caribbean, comparative phylogeography, connectivity, continental-shelf, fish metapopulations, Individual-based model, larval dispersal, marine populations, microsatellites, Montastraea annularis, multilocus genotype data, reef-building corals, subdivided populations, taxonomic distinctness},
doi = {10.1111/j.1365-294X.2012.05455.x},
pmid = {22276913},
language = {English},
rating = {0},
date-added = {2013-08-21T06:36:40GMT},
date-modified = {2016-06-07T03:58:54GMT},
abstract = {Understanding patterns of connectivity among populations of marine organisms is essential for the development of realistic, spatially explicit models of population dynamics. Two approaches, empirical genetic patterns and oceanographic dispersal modelling, have been used to estimate levels of evolutionary connectivity among marine populations but rarely have their potentially complementary insights been combined. Here, a spatially realistic Lagrangian model of larval dispersal and a theoretical genetic model are integrated with the most extensive study of gene flow in a Caribbean marine organism. The 871 genets collected from 26 sites spread over the wider Caribbean subsampled 45.8% of the 1900 potential unique genets in the model. At a coarse scale, significant consensus between modelled estimates of genetic structure and empirical genetic data for populations of the reef-building coral Montastraea annularis is observed. However, modelled and empirical data differ in their estimates of connectivity among northern Mesoamerican reefs indicating that processes other than dispersal may dominate here. Further, the geographic location and porosity of the previously described east-west barrier to gene flow in the Caribbean is refined. A multi-prong approach, integrating genetic data and spatially realistic models of larval dispersal and genetic projection, provides complementary insights into the processes underpinning population connectivity in marine invertebrates on evolutionary timescales.},
url = {http://doi.wiley.com/10.1111/j.1365-294X.2012.05455.x},
