affiliation = {[Berumen, Michael L.; Saenz-Agudelo, Pablo] King Abdullah Univ Sci {\&} Technol, Red Sea Res Ctr, Thuwal 23955, Saudi Arabia. [Berumen, Michael L.; Thorrold, Simon R.] Woods Hole Oceanog Inst, Dept Biol, Woods Hole, MA 02540 USA. [Almany, Glenn R.; Jones, Geoffrey P.] James Cook Univ, ARC Ctr Excellence Coral Reef Studies, Townsville, Qld 4811, Australia. [Almany, Glenn R.; Jones, Geoffrey P.] James Cook Univ, Sch Marine {\&} Trop Biol, Townsville, Qld 4811, Australia. [Planes, Serge] Ctr Rech Insulaires {\&} Observ Environm CRIOBE, EPHE, CNRS, USR 3278, Papetoai 98729, Moorea, Fr Polynesia. [Planes, Serge] Lab Excellence CORAIL, Papetoai 98729, Moorea, Fr Polynesia.Berumen, ML (reprint author), King Abdullah Univ Sci {\&} Technol, Red Sea Res Ctr, Bldg 2,Room 3221, Thuwal 23955, Saudi Arabia.michael.berumen@kaust.edu.sa},
keywords = {Amphiprion percula, Chaetodon vagabundus, connectivity, CONSERVATION, coral reef fish, Dispersal, fisheries, gene flow, larval, management, Marine Protected Areas, microsatellite parentage analysis, Networks, parentage analysis, population connectivity, replenishment, Reserve, self-recruitment},
