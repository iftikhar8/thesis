@article{Thorrold:2002tz,
author = {Thorrold, Simon R and Jones, Geoffrey P and Hellberg, M E and Burton, R S and Swearer, Stephen E and Neigel, J E and Morgan, Steven G and Warner, Robert R},
title = {{Quantifying larval retention and connectivity in marine populations with artificial and natural markers}},
journal = {Bulletin of Marine Science},
year = {2002},
volume = {70},
number = {1},
pages = {291--308},
month = jan,
annote = {ISI Document Delivery No.: 565NZTimes Cited: 107Cited Reference Count: 83Thorrold, SR Jones, GP Hellberg, ME Burton, RS Swearer, SE Neigel, JE Morgan, SG Warner, RRRosenstiel sch mar atmos sciMiamiS},
affiliation = {Woods Hole Oceanog Inst, Dept Biol, Woods Hole, MA 02543 USA. James Cook Univ N Queensland, Sch Marine Biol {\&} Aquaculture, Townsville, Qld 4811, Australia. Louisiana State Univ, Dept Biol Sci, Baton Rouge, LA 70802 USA. Univ Calif San Diego, Scripps Inst Oceanog, Div Marine Biol Res, La Jolla, CA 92093 USA. Univ Calif Santa Barbara, Dept Ecol Evolut {\&} Marine Biol, Santa Barbara, CA 93106 USA. Univ Louisiana, Dept Biol, Lafayette, LA 70504 USA. Univ Calif Bodega, Marine Lab, Bodega Bay, CA 94923 USA.Thorrold, SR (reprint author), Woods Hole Oceanog Inst, Dept Biol, Woods Hole, MA 02543 USA.},
keywords = {biological tags, coral reef fish, genetic-heterogeneity, invertebrate larvae, juvenile salmonids, marking, morone-saxatilis, radioactive strontium sr-85, striped bass, vertebral number},
language = {English},
rating = {0},
date-added = {2013-08-21T06:36:58GMT},
date-modified = {2016-07-30T05:44:34GMT},
abstract = {Quantifying larval retention and connectivity remains a major hurdle in the development of realistic spatially-explicit population models in marine systems. This lack of knowledge is primarily due to the difficulty of conducting mark-recapture studies in species that are characterized by the production of large numbers of small pelagic offspring that suffer high initial mortality rates. Advances in artificial and natural tagging methodologies have, however, significantly increased the ability of marine ecologists to track larvae throughout the pelagic larval phase and subsequent recruitment into benthic populations. Many of these empirical approaches are now possible with the development of DNA sequencing and mass spectrometric instrumentation in the last decade. The presence of artificial tags in recaptured individuals remains the only unequivocal method for marking marine larvae. However, the difficulties of tagging sufficient numbers of larvae, with negligible handling effects, are formidable. Natural tags, including genetic markers and geochemical signatures in calcified tissues, are rarely unique indicators of source location, but have a significant advantage because all larvae released from an area are indelibly tagged, Given the strengths and limitations of the techniques, an approach that combines two or more techniques will likely be necessary to quantify larval retention and connectivity over appropriate spatio-temporal scales. Where possible, such a multi-technique strategy should include both artificial and natural tags.},
uri = {\url{papers3://publication/uuid/84426D27-A6B1-4E52-B79D-C278E2176709}}
}

