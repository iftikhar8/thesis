keywords = {ABUNDANCE, africa, Bahia de La Paz, circadian rhythms, distribution, fish larvae, Gulf of California, Mexico, SEASONAL-VARIATIONS, st-lucia estuary, variability, vertical},
doi = {10.1016/j.ecss.2007.08.016},
language = {English},
