@article{Booth:2007ir,
author = {Booth, David J and Figueira, Will Franklin and Gregson, M A and Brown, L and Beretta, G},
title = {{Occurrence of tropical fishes in temperate southeastern Australia: Role of the East Australian Current}},
journal = {Estuarine Coastal and Shelf Science},
year = {2007},
