annote = {ISI Document Delivery No.: 145JZTimes Cited: 26Cited Reference Count: 46Booth, D. J. Figueira, W. F. Gregson, M. A. Brown, L. Beretta, G.Academic press ltd elsevier science ltdLondon},
affiliation = {Univ Technol Sydney, Dept Environm Sci, Sydney, NSW 2007, Australia.Figueira, WF (reprint author), Univ Technol Sydney, Dept Environm Sci, POB 123, Sydney, NSW 2007, Australia.william.figueira@uts.edu.au},
keywords = {[29 degrees S-38 degrees S, 149 degrees E-155 degrees E], Australia, boundaries, Butterflyfish, connectivity, coral-reef fishes, damselfish, damselfishes, East Australian Current, growth, larval duration, Marine, NEW-SOUTH-WALES, PATTERNS, Pomacentridae, population-structure, processes, regional, settlement, sustained swimming abilities, Transport, tropical fish, western},
doi = {10.1016/j.ecss.2006.10.003},
language = {English},
rating = {0},
date-added = {2013-08-21T06:36:39GMT},
date-modified = {2016-06-07T04:00:08GMT},
abstract = {Dispersal of larval propagules is the major mechanism facilitating connectivity of marine populations. However, only a fraction of larvae settle in suitable habitat. For coral reef fishes, many larvae are advected away from coral reefs, often despite strong behavioural mechanisms (including swimming), and some may travel long distances away from the tropics. Here we document the occurrence of tropical reef fishes along the southeast coast of Australia between 2003 and 2005 and evaluate the role of the East Australian Current (EAC) in driving this pattern. In total we observed 47 species of tropical fishes from 11 families during the summer recruitment season (January to May) at locations spanning most of the length of the New South Wales coast (28 degrees S-37.5 degrees S latitude, similar to 1700 km from the southern end of the Great Barrier Reef). Southern locations tended to have reduced richness and density relative to northern ones. In general, the southward extent of distribution of the most commonly observed species was well explained by their planktonic larval durations. Recruitment events tended to be much more episodic in Merimbula (37 degrees S) than Sydney (34 degrees S), but there was little evidence for interannual similarity in the spatial patterns of recruitment of individual species with exception of the numerical dominance of Abudefduf vaigiensis and Abudefduf sexfasciatus (Pomacentridae) at the Sydney location and of Chaetodon auriga and Chaetodon flavirostris (Chaetodontidae) at the Merimbula location. Despite strong evidence for the role of the EAC in the transport of these species at a coastal scale, we found little evidence that individual recruitment events were correlated with local increases in water temperature that would be associated with EAC ingress. (c) 2006 Elsevier Ltd. All rights reserved.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0272771406004859},
local-url = {file://localhost/Users/steve/Dropbox/Research/Library.papers3/Files/3C/3C7647B5-7AC1-4753-A97A-E65B12565C32.pdf},
file = {{3C7647B5-7AC1-4753-A97A-E65B12565C32.pdf:/Users/steve/Dropbox/Research/Library.papers3/Files/3C/3C7647B5-7AC1-4753-A97A-E65B12565C32.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.ecss.2006.10.003}}
}

